{
	"version": 1,
	"title": "System Assets",
	"description": "Engine specific assets",
	"assetCompiler":
	{
		"defaultBuildConfig": "win64_development",
		"buildConfigs":
		{
			"win64_development":
			{
				"platform": "win64",
				"buildType": "development",
				"outputPath": "../data/development/",
				"deployPath": "../data/development/",
				"processorConfigs":
				{
					"texture":
					{
						"format": "rgba8"
					}
				},
				"excludeFileMasks":
				[
					"*/textures/sky/*.png"
				]
			},
			"win64_shipping":
			{
				"platform": "win64",
				"buildType": "shipping",
				"outputPath": "../data/shipping/",
				"deployPath": "../data/shipping/",
				"processorConfigs":
				{
					"texture":
					{
						"format": "rgba8"
					}
				},
				"excludeFileMasks":
				[
					"*/textures/sky/*.png"
				]
			}
		},
		"bundles":
		{
			"engine":
			{
				"path": ""
			}
		},
		"defaultBundleName": "engine"
	}
}